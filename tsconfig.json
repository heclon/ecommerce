{
  "compilerOptions": {
    "skipLibCheck": true,
    "jsx": "preserve",
    "target": "ES2020",
    "module": "commonjs",
    "lib": ["dom", "dom.iterable", "esnext"],
    "strict": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "useUnknownInCatchVariables": false
  }
}
