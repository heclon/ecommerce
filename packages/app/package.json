  "name": "@ecommerce/app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "yarn run generate && next build",
    "build-storybook": "graphql-codegen && NODE_ENV=test yarn run -T build-storybook",
    "build:replace": "replace-in-file prisma.SortOrder prisma.Prisma.SortOrder **/nexus.ts && replace-in-file prisma.QueryMode prisma.Prisma.QueryMode **/nexus.ts",
    "chromatic:exec": "yarn generate && yarn run -T chromatic --project-token=20235bf1d524",
    "predev": "yarn run generate",
    "dev": "yarn run -T npm-run-all --parallel dev:next dev:nexus dev:graphql-codegen",
    "dev:graphql-codegen": "graphql-codegen --watch",
    "dev:next": "next dev",
    "dev:nexus": "yarn run -T ts-node-dev --exit-child --no-notify --respawn --require dotenv/config --transpile-only -P nexus.tsconfig.json server/schema/index.ts",
    "generate": "yarn run generate:nexus && yarn run generate:graphql-codegen && yarn run build:replace",
    "generate:graphql-codegen": "graphql-codegen",
    "generate:nexus": "yarn run -T ts-node --transpile-only -P nexus.tsconfig.json server/schema/index.ts",
    "prod": "next build && next start",
    "start": "next start",
    "storybook": "yarn run -T start-storybook -p 6007",
    "test": "jest --runInBand",
    "test:acceptance:coverage": "yarn run -T start-server-and-test 'npx nyc --reporter=lcov --reporter=json next dev' 3000 'yarn run test:acceptance'",
    "test:acceptance": "yarn run -T playwright test --retries 2 --max-failures 3",
    "test:acceptance:debug": "yarn run -T playwright test --retries 0 --max-failures 1 --debug"
  },
